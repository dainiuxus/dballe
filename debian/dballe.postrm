#!/bin/sh

#set +C # turn off noclobber

echo -n "Removing indexed tables..."
for i in dballe B000101 B000103 B000203 D000101 D000103 D000203
do
	rm -f /var/cache/dballe/$i.txt
done
echo "done."

if [ -d /var/cache/dballe ] && [ "`echo /var/cache/dballe/*`" = '/var/cache/dballe/*' ]
then
	echo "Removing empty directory /var/cache/dballe"
	rmdir /var/cache/dballe
fi

#DEBHELPER#
